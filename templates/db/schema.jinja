generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

{% for model in models %}

model {{ model.id }} {
  {%- autoescape false -%}
    {% for field in model.fields %}
    {{ field.id }} {{ field.type }} {{ field.attrs }}
    {% endfor %}
  {% endautoescape -%}
}

{% endfor %}
