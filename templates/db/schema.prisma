generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "{{ datasource_provider }}"
  url      = "{{ datasource_url }}"
}

{% for model in models %}

model {{ model.id }} {
  {% for field in model.fields %}
  {{ field.id }} {{ field.type }} {{ field.attrs }}
  {% endfor %}
}

{% endfor %}
