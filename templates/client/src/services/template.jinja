const baseUrl = 'http://localhost:4000';

{% for func in functions %}
export const {{ func.id }} = (
  {% if func.type == "update" or func.type == "delete" or func.type == "findFirst" %}
  id: number
  {% endif %} ) =>
  `${baseUrl}/{{ func.route }}/{% if func.type == "update" or func.type == "delete" or func.type == "findFirst" %}${id}{% endif %}`
{% endfor %}
