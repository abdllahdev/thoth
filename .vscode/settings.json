{
  "emeraldwalk.runonsave": {
    "commands": [
      {
          "match": ".$ml",
          "cmd": "dune build"
      }
    ]
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "ocamllabs.ocaml-platform",
  "ocaml.sandbox": {
    "kind": "opam",
    "switch": "default"
  },
  "files.associations": {
    "*.jinja": "jinja",
  }
}
